
{% extends 'BorrowersHomeBundle::layout.html.twig' %}

{% block content %}

<div class="span2">
<a class="btn btn-info" href="{{ path('file') }}">List</a>
<a class="btn btn-primary" href="{{ path('file_edit', { 'id': entity.id }) }}">Edit</a>
 
</div>       
  
<div class="span1">        
<form action="{{ path('file_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button class="btn btn-danger" type="submit">Delete</button>
</form>
</div>
  

        

<div class="span6">    
<table class="table table-bordered table-condensed table-striped">
<tbody>        
    <tr>
        <th>Id</th>         
        <td>{{ entity.id }}</td>         
    </tr>        
    <tr>
        <th>Path</th>         
        <td>{{ entity.path }}</td>         
    </tr>        
    <tr>
        <th>Title</th>         
        <td>{{ entity.title }}</td>         
    </tr>        
    <tr>
        <th>Courseid</th>         
        <td>{{ entity.courseid }}</td>         
    </tr>        
    <tr>
        <th>Projectid</th>         
        <td>{{ entity.projectid }}</td>         
    </tr>        
    <tr>
        <th>Stageid</th>         
        <td>{{ entity.stageid }}</td>         
    </tr>        
    <tr>
        <th>Userid</th>         
        <td>{{ entity.userid }}</td>         
    </tr>        
    <tr>
        <th>Essayid</th>         
        <td>{{ entity.essayid }}</td>         
    </tr>        
    <tr>
        <th>Created</th>        <td>{{ entity.created|date('Y-m-d H:i:s') }}</td>         
    </tr>        
    <tr>
        <th>Updated</th>        <td>{{ entity.updated|date('Y-m-d H:i:s') }}</td>         
    </tr>  
</tbody>
</table>
</div>          
{% endblock %}
